// ðŸ”§ ConfiguraÃ§Ãµes do Prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

// ==============================
// ðŸ‘¤ Modelos: UsuÃ¡rio
// ==============================
model Usuario {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  inserted_at  DateTime @default(now())
  updated_at DateTime @updatedAt
}
